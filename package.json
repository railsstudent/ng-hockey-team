{
  "name": "ng-hockey-team",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/ng-hockey-team/stats.json",
    "prettier-check": "prettier --single-quote --list-different \"*.{js,json,css,md,ts,html,component.html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@clr/angular": "^2.3.4",
    "@clr/icons": "^2.3.3",
    "@clr/ui": "^2.3.3",
    "@netbasal/ngx-content-loader": "^2.2.0",
    "@ngrx/effects": "^8.5.2",
    "@ngrx/entity": "^8.5.2",
    "@ngrx/router-store": "^8.5.2",
    "@ngrx/store": "^8.5.2",
    "@ngrx/store-devtools": "^8.5.2",
    "@types/uuid": "^3.4.4",
    "@webcomponents/custom-elements": "^1.0.0",
    "add": "^2.0.6",
    "core-js": "^2.5.4",
    "date-fns": "^2.9.0",
    "prettier": "1.19.1",
    "rxjs": "~6.5.3",
    "rxjs-tslint-rules": "^4.23.1",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "yarn": "^1.15.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@ngrx/schematics": "^8.6.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.6.1",
    "codelyzer": "^5.0.1",
    "husky": "3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "8.1.5",
    "protractor": "~5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ng lint && ng build --prod"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,html,component.html}": [
      "npm run prettier",
      "git add"
    ]
  },
  "resolutions": {}
}